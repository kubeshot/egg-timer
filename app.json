{
  "expo": {
    "name": "egg-timer",
    "slug": "egg-timer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "experiments": {
      "baseUrl": "/egg-timer"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["fetch"]
      }
    },
    "android": {
      "permissions": ["INTERNET", "RECEIVE_BOOT_COMPLETED", "WAKE_LOCK"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ansh_kubeshot.eggtimer"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-background-fetch",
        {
          "startOnBoot": true
        }
      ]
    ]
  }
}